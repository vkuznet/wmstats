<!DOCTYPE html>
<html>
<head>
    <title>WMStats server</title>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="icon" type="image/jpg" href="{{.Base}}/images/cms_logo.jpg" />
    <link rel="stylesheet" href="{{.Base}}/css/main.css" />
    <link rel="stylesheet" type="text/css" href="{{.Base}}/css/kube.min.css"/>
    <link rel="stylesheet" type="text/css" href="{{.Base}}/css/kube-addons.min.css"/>
    <link rel="stylesheet" href="{{.Base}}/css/main-page.css" />
    <script type="text/javascript" src="{{.Base}}/js/kube.min.js"></script>
    <script type="text/javascript" src="{{.Base}}/js/kube-addons.min.js"></script>
    <script type="text/javascript" src="{{.Base}}/js/utils.js"></script>
    <script>$K.dom(window).on('load', $K.init);</script>
</head>
<body>


		<div class="page">
            <header class="header">
                {{.Header}}
            </header>
			<main class="main is-container">
				<div class="main-sidebar">
                    {{.Menu}}
                </div>
				<div class="main-content">
                    <div class="is-row">
                        <div class="is-col is-80">
                        {{.Search}}
                        </div>
                        <div class="is-col is-20">
                        {{.Filter}}
                        </div>
                    </div>
                    {{.Table}}
                </div>
			</main>
			<footer class="footer">
                {{.Footer}}
            </footer>
		</div>

</body>
</html>
